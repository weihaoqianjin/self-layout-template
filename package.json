{
  "name": "sass-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "?_testdev": "本地开发时，加载测试环境配置",
    "testdev": "vue-cli-service serve --mode test",
    "?_proddev": "本地开发时，加载正式环境配置",
    "proddev": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build",
    "?_testbuild": "按照测试环境配置打包项目,用做自动化部署时测试环境的打包命令",
    "testbuild": "vue-cli-service build --mode test",
    "?_devbuild": "按照开发环境配置打包项目, 用做自动化部署时开发环境的打包命令",
    "devbuild": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix src/",
    "report": "vue-cli-service build --report",
    "inspect": "vue inspect > output.js"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "nprogress": "^0.2.0",
    "qs": "^6.8.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^2.8.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
